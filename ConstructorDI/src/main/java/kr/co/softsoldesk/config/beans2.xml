<?xml version="1.0" encoding="UTF-8"?>

<beans xsi:schemaLocation="http://www.springframework.org/schema/beans
							http://www.springframework.org/schema/beans/spring-beans.xsd"
							xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
							xmlns="http://www.springframework.org/schema/beans"
							default-init-method="default_init" default-destroy-method="default_destroy">
							
	<bean id="tv" class="kr.co.softsoldesk.beans.SamsungTV" scope="prototype">
		<constructor-arg ref="speaker" index="0"/>
		<constructor-arg type="int" value="27000000" index="1"/>
	</bean>	
	<bean class="kr.co.softsoldesk.beans.SonySpeaker" id="speaker" scope="prototype"/>
</beans>
